    <!-- 실습 get으로 정보 받기 -->
    <form action="/getForm" method="get">
        이름 <input type="text" name="name">
        <fieldset>
            <legend>성별</legend>
            <input type="radio" name="man"> 남자
            <input type="radio" name="woman"> 여자
        </fieldset>
        <fieldset>
            <legend>생년월일</legend>
            <select name="year">
                <option value="2010">2010</option>
            </select>
            년
            <select name="month">
                <option value="1">1</option>
            </select>
            월
            <select name="day">
                <option value="1">1</option>
            </select>
            일
        </fieldset>
        <fieldset>
            <legend>관심사</legend>
            <input type="checkbox" name="trip"> 여행
            <input type="checkbox" name="fashion"> 패션
            <input type="checkbox" name="food"> 음식
        </fieldset>
        <button>회원가입</button>
    </form>
